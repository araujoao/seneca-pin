    "annotate": "docco pin.js -o docs/annotated"
  },

  "dependencies": {
    "jsonic": "0.2.2",
    "lodash": "4.15.0"
  },
  "devDependencies": {
    "code": "3.0.x",
    "coveralls": "2.11.x",
    "docco": "0.7.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-standard": "2.x.x",
    "lab": "11.0.x",
    "seneca": "plugin"
  },
  "files": [
    "pin.js",
    "README.md",
    "LICENSE"
  ]
}
